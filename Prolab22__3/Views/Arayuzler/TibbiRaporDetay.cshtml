@namespace Prolab22__3.Views.Arayuzler
@model Prolab22__3.Models.TibbiRapor

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tıbbi Rapor Detayları</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <h1>Tıbbi Rapor Detayları</h1>
    <div>
        <p><strong>Rapor Tarihi:</strong> @Model.RaporTarihi</p>
        <p><strong>Rapor İçeriği:</strong> @Model.RaporIcerigi</p>
        <p><strong>Rapor URL:</strong> @Model.URL</p>
        <p><strong>Hasta ID:</strong> @Model.HastaID</p>
        <p><strong>Doktor ID:</strong> @Model.DoktorID</p>
    </div>
</body>
</html>
